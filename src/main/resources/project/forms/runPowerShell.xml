<editor>
    <help>https://docs.cloudbees.com/docs/cloudbees-cd/latest/plugins/ec-powershell#runPowerShell</help>
    <formElement>
        <type>entry</type>
        <label>Path to a powershell file:</label>
        <property>powershellFile</property>
        <documentation>Provide an absolute path to a powershell script file (.ps1).</documentation>
        <required>0</required>
    </formElement>
    <formElement>
        <type>textarea</type>
        <label>Command:</label>
        <property>powershellCommand</property>
        <documentation>Enter powershell commands directly.</documentation>
        <required>0</required>
    </formElement>
</editor>
